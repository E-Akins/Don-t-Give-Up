{
    while(!place_meeting(x,y+sign(vspd),obj_ground)) y += sign(vspd)
    vspd = 0
}
y += vspd

// Shrink to normal size
if (shrink &amp;&amp; !gameover) if (alarm[1] == -1) alarm[1] = 2

// Game Over Animation
if (gameover)
{
    image_angle += 20
    image_angle = clamp(image_angle,0,180)   
